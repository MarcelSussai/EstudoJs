{
    "files.insertFinalNewline": true,
    "flow.runOnEdit": false,
    "html.format.endWithNewline": true,
    "editor.trimAutoWhitespace": false,
    // "eslint.enable": false,
    "jshint.enable": false,
    // "tslint.enable": false,
    "editor.tabSize": 2,
    "better-comments.tags": [
        {
            "tag": "!",
            "color": "#FF2D00",
            "strikethrough": false,
            "backgroundColor": "transparent",
        },
        {
            "tag": "?",
            "color": "#549491",
            "strikethrough": false,
            "backgroundColor": "transparent"
        },
        {
            "tag": "//",
            "color": "#474747",
            "strikethrough": true,
            "backgroundColor": "transparent"
        },
        {
            "tag": "todo",
            "color": "#FF8C00",
            "strikethrough": false,
            "backgroundColor": "transparent"
        },
        {
            "tag": "*",
            "color": "#98C379",
            "strikethrough": false,
            "backgroundColor": "transparent"
        },
        {
            "tag": "#",
            "color": "#B2AC37",
            "strikethrough": false,
            "backgroundColor": "transparent"
        }
    ],
    "eslint.enable": false,
    "jshint.enable": false,
    "tslint.enable": false
}
